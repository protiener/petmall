/* pages/uptocommunity/uptocommunity.wxss */
.up-nav{
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 30rpx;
}
.up-content{
  width: 90%;
  margin: 2% 5%;
}
.content-title{
  height: 40rpx;
  border-bottom: solid 2rpx gainsboro;
  padding: 10rpx 0;
  font-size: 30rpx;
}
.content-text{
  width: 100%;
  padding: 10rpx 0;
  height: 200rpx;
  font-size: 26rpx;
}
.upbotton{
  border: solid 2rpx gainsboro;
  width: 160rpx;
  height: 160rpx;
  font-size: 100rpx;
  text-align: center;
  line-height: 140rpx;
  color: gainsboro;
  font-weight: lighter;
}

.up-image {
  display: flex;
  width: 90%;
  margin-left: 5%;
}
.up-image {
  display: flex;
  flex-wrap: wrap;
}
.up-image image{
  width: 160rpx;
  height: 160rpx;
  margin-bottom: 10rpx;
  margin-right: 8rpx;
}
.deletebotton{
  float: right;
  color: gray;
  z-index: 999;
  width: 30rpx;
  height: 30rpx;
  top: -160rpx;
}
.inputbotton{
  width: 90%;
  border-radius: 20rpx;
  margin: 20rpx 5%;
  background-color: #ff5557;
  color: white;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  font-weight: bold
}